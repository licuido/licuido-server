{
	"name": "fastify-sequelize-ts",
	"version": "1.0.0",
	"description": "",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"prisma": {
		"schema": "./prisma/schema.prisma",
		"seed": "ts-node prisma/seeders/index.ts"
	},
	"scripts": {
		"test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
		"start": "npm run build:ts && fastify start -a '0.0.0.0' -l info dist/app.js",
		"build:ts": "rm -rf dist && tsc && tsc-alias -p tsconfig.json",
		"watch:ts": "tsc -w",
		"dev": "nodemon",
		"dev:start": "fastify start  -a '0.0.0.0' --ignore-watch=.ts$ -w -l info -P dist/app.js",
		"prepare": "husky install",
		"model": "node ./dist/scripts/auto.js",
		"migrate": "npx prisma migrate deploy",
		"seed": "npx prisma db seed",
		"migrate:create": "npx prisma migrate dev --create-only"
	},
	"keywords": [
		"fastify",
		"sequelize",
		"typescript"
	],
	"author": "ray",
	"license": "ISC",
	"dependencies": {
		"@crayond_dev/alertshub-client-sdk": "^1.0.0",
		"@fastify/autoload": "^5.0.0",
		"@fastify/cors": "^8.2.0",
		"@fastify/helmet": "^10.1.0",
		"@fastify/multipart": "^7.4.1",
		"@fastify/rate-limit": "^8.0.0",
		"@fastify/sensible": "^5.0.0",
		"@fastify/swagger": "^8.3.1",
		"@fastify/swagger-ui": "^1.4.0",
		"@prisma/client": "^5.12.1",
		"axios": "^1.6.8",
		"fastify": "^4.0.0",
		"fastify-plugin": "^4.0.0",
		"fluent-json-schema": "^4.2.1",
		"moment": "^2.30.1",
		"pg": "^8.9.0",
		"sequelize": "^6.29.0"
	},
	"devDependencies": {
		"@fastify/jwt": "^8.0.0",
		"@types/dotenv": "^8.2.0",
		"@types/node": "^18.0.0",
		"@types/pg": "^8.11.5",
		"@types/tap": "^15.0.5",
		"concurrently": "^7.0.0",
		"dotenv": "^16.0.3",
		"fastify-cli": "^5.7.1",
		"fastify-tsconfig": "^1.0.1",
		"husky": "^8.0.0",
		"pretty-quick": "^3.1.3",
		"sequelize-auto": "^0.8.8",
		"tap": "^16.1.0",
		"ts-node": "^10.4.0",
		"tsc-alias": "^1.8.8",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^4.5.4"
	}
}
